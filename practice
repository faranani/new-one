var name = "Chachu";
var age  = 25;
 while(age<=25)
 {
    for(let i=0; i<10;i ++)
    {
        if (age === 25 && age<= 50) 
        {
            name = "Thomas";
        }
        else 
        {
            name = "Chachu";
        }
    }
    
    
console.log("Welcome to " + name);
console.log("Welcome to my World");
console.log("Reneilwe Sesoko piece of code")
console.log(" you are getting old")
 }




